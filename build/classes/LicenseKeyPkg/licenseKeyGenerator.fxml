<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LicenseKeyPkg.LicenseKeyGeneratorController">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="8.0">
         <children>
            <HBox>
               <children>
                  <Label prefHeight="30.0" prefWidth="125.0" text=" Produce ID">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="textFieldProduceId" prefHeight="30.0" prefWidth="400.0" promptText="0123456789">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="30.0" prefWidth="125.0" text="License Type">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBoxLicenseType" prefHeight="30.0" prefWidth="400.0" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="30.0" prefWidth="125.0" text="Experation Day">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <DatePicker fx:id="datePickerDate" prefHeight="30.0" prefWidth="400.0" promptText="2022-10-19" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="30.0" prefWidth="125.0" text="Product key">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="textFieldProduceKey" prefHeight="30.0" prefWidth="400.0" promptText="AAAAA-AAXXX-XXXXX-XXXXX-XXXXX">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT">
               <children>
                  <Button fx:id="buttonGenerate" mnemonicParsing="false" text="Generate">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
